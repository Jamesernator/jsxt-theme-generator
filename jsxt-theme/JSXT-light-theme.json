{
    "$schema": "vscode://schemas/color-theme",
    "name": "JSXT Light",
    "type": "light",
    "colors": {
        "activityBar.activeBackground": "#FFF",
        "activityBar.activeBorder": "#0AF",
        "activityBar.background": "#CCC",
        "activityBar.foreground": "#222",
        "editor.background": "#EEE",
        "editor.foreground": "#222",
        "focusBorder": "#888",
        "foreground": "#444",
        "list.activeSelectionBackground": "#0AF",
        "selection.background": "#0AF",
        "statusBar.background": "#0AF",
        "statusBar.foreground": "#FFF",
        "terminal.ansiBlack": "#444",
        "terminal.ansiBlue": "#00F",
        "terminal.ansiBrightBlack": "#777",
        "terminal.ansiBrightBlue": "#0AF",
        "terminal.ansiBrightCyan": "#0CC",
        "terminal.ansiBrightGreen": "#0C3",
        "terminal.ansiBrightMagenta": "#F0F",
        "terminal.ansiBrightRed": "#F00",
        "terminal.ansiBrightWhite": "#FFF",
        "terminal.ansiBrightYellow": "#CC0",
        "terminal.ansiCyan": "#0AA",
        "terminal.ansiGreen": "#0A0",
        "terminal.ansiMagenta": "#C0C",
        "terminal.ansiRed": "#C00",
        "terminal.ansiWhite": "#EEE",
        "terminal.ansiYellow": "#BB0",
        "terminal.background": "#EEE",
        "terminal.foreground": "#333",
        "terminalCursor.background": "#222",
        "terminalCursor.foreground": "#AAA",
        "widget.shadow": "#555"
    },
    "semanticHighlighting": true,
    "semanticTokenColors": {
        "attribute": {
            "foreground": "#777"
        },
        "boolean": {
            "foreground": "#777"
        },
        "class": {
            "bold": true,
            "foreground": "#0C3"
        },
        "class.declaration": {
            "underline": true
        },
        "comment": {
            "foreground": "#777"
        },
        "constant": {
            "foreground": "#777"
        },
        "enum": {
            "bold": true,
            "foreground": "#0C3"
        },
        "enum.declaration": {
            "underline": true
        },
        "enumMember": {
            "foreground": "#0AF"
        },
        "enumMember.declaration": {
            "underline": true
        },
        "function": {
            "foreground": "#777"
        },
        "function.declaration": {
            "underline": true
        },
        "interface": {
            "bold": true,
            "foreground": "#0C3"
        },
        "interface.declaration": {
            "underline": true
        },
        "keyword": {
            "bold": true,
            "foreground": "#0AF"
        },
        "keyword.expression": {
            "foreground": "#A0F"
        },
        "keyword.operator": {
            "bold": true,
            "foreground": "#777"
        },
        "keyword.storage": {
            "bold": true,
            "foreground": "#A0F",
            "italic": true
        },
        "label": {
            "foreground": "#777"
        },
        "method": {
            "foreground": "#777"
        },
        "method.declaration": {
            "underline": true
        },
        "namespace": {
            "bold": true,
            "foreground": "#0C3"
        },
        "namespace.declaration": {
            "underline": true
        },
        "number": {
            "foreground": "#777"
        },
        "operator": {
            "bold": true,
            "foreground": "#777"
        },
        "operator.assignment": {
            "bold": true,
            "foreground": "#777"
        },
        "parameter": {
            "foreground": "#F0F"
        },
        "parameter.declaration": {
            "underline": true
        },
        "property": {
            "foreground": "#0AF"
        },
        "property.declaration": {
            "underline": true
        },
        "punctuation": {
            "bold": true,
            "foreground": "#777",
            "underline": false
        },
        "regexp": {
            "foreground": "#F80"
        },
        "regexp.characterClass": {
            "foreground": "#777"
        },
        "string": {
            "foreground": "#F80"
        },
        "string.escape": {
            "foreground": "#777"
        },
        "tag": {
            "bold": true,
            "foreground": "#777"
        },
        "text": {
            "foreground": "#777"
        },
        "type": {
            "bold": true,
            "foreground": "#0C3"
        },
        "type.declaration": {
            "underline": true
        },
        "type.primitive": {
            "foreground": "#777"
        },
        "typeParameter": {
            "bold": true,
            "foreground": "#0C3"
        },
        "typeParameter.declaration": {
            "underline": true
        },
        "variable": {
            "foreground": "#F0F"
        },
        "variable.declaration": {
            "underline": true
        }
    },
    "tokenColors": [
        {
            "scope": "entity.other.attribute-name",
            "settings": {
                "foreground": "#777"
            }
        },
        {
            "scope": "constant.language.boolean",
            "settings": {
                "foreground": "#777"
            }
        },
        {
            "scope": "entity.name.type.class",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#0C3"
            }
        },
        {
            "scope": "meta.class entity.other.inherited-class",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#0C3"
            }
        },
        {
            "scope": "meta.class entity.name.type.class",
            "settings": {
                "fontStyle": "bold underline",
                "foreground": "#0C3"
            }
        },
        {
            "scope": "comment",
            "settings": {
                "foreground": "#777"
            }
        },
        {
            "scope": "constant.language",
            "settings": {
                "foreground": "#777"
            }
        },
        {
            "scope": "entity.name.type.enum",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#0C3"
            }
        },
        {
            "scope": "meta.enum.declaration entity.name.type.enum",
            "settings": {
                "fontStyle": "bold underline",
                "foreground": "#0C3"
            }
        },
        {
            "scope": "variable.other.enummember",
            "settings": {
                "foreground": "#0AF"
            }
        },
        {
            "scope": "meta.enum.declaration variable.other.enummember",
            "settings": {
                "fontStyle": "underline",
                "foreground": "#0AF"
            }
        },
        {
            "scope": "entity.name.function",
            "settings": {
                "foreground": "#777"
            }
        },
        {
            "scope": "support.function",
            "settings": {
                "foreground": "#777"
            }
        },
        {
            "scope": "meta.definition.function entity.name.function",
            "settings": {
                "fontStyle": "underline",
                "foreground": "#777"
            }
        },
        {
            "scope": "entity.name.type.interface",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#0C3"
            }
        },
        {
            "scope": "meta.interface entity.other.inherited-class",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#0C3"
            }
        },
        {
            "scope": "meta.interface entity.name.type.interface",
            "settings": {
                "fontStyle": "bold underline",
                "foreground": "#0C3"
            }
        },
        {
            "scope": "keyword.other",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#0AF"
            }
        },
        {
            "scope": "variable.language",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#A0F"
            }
        },
        {
            "scope": "keyword.operator.new",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#777"
            }
        },
        {
            "scope": "keyword.operator.expression",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#777"
            }
        },
        {
            "scope": "storage",
            "settings": {
                "fontStyle": "bold italic",
                "foreground": "#A0F"
            }
        },
        {
            "scope": "keyword.control",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#0AF"
            }
        },
        {
            "scope": "keyword.operator.expression.of",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#0AF"
            }
        },
        {
            "scope": "entity.name.label",
            "settings": {
                "foreground": "#777"
            }
        },
        {
            "scope": "meta.method.declaration meta.definition.method entity.name.function",
            "settings": {
                "fontStyle": "underline",
                "foreground": "#777"
            }
        },
        {
            "scope": "entity.name.type.module",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#0C3"
            }
        },
        {
            "scope": "meta.namespace.declaration entity.name.type.module",
            "settings": {
                "fontStyle": "bold underline",
                "foreground": "#0C3"
            }
        },
        {
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#777"
            }
        },
        {
            "scope": "keyword.operator",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#777"
            }
        },
        {
            "scope": "punctuation.definition.group",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#777"
            }
        },
        {
            "scope": "keyword.operator.assignment",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#777"
            }
        },
        {
            "scope": "keyword.operator.bitwise",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#777"
            }
        },
        {
            "scope": "keyword.operator.logical",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#777"
            }
        },
        {
            "scope": "keyword.operator.arithmetic",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#777"
            }
        },
        {
            "scope": "keyword.operator.comparison",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#777"
            }
        },
        {
            "scope": "variable.parameter",
            "settings": {
                "foreground": "#F0F"
            }
        },
        {
            "scope": "meta.parameters variable.parameter",
            "settings": {
                "fontStyle": "underline",
                "foreground": "#F0F"
            }
        },
        {
            "scope": "variable.other.property",
            "settings": {
                "foreground": "#0AF"
            }
        },
        {
            "scope": "variable.object.property",
            "settings": {
                "foreground": "#0AF"
            }
        },
        {
            "scope": "support.variable.property",
            "settings": {
                "foreground": "#0AF"
            }
        },
        {
            "scope": "support.type.property-name",
            "settings": {
                "foreground": "#0AF"
            }
        },
        {
            "scope": "meta.object-literal.key",
            "settings": {
                "fontStyle": "underline",
                "foreground": "#0AF"
            }
        },
        {
            "scope": "meta.object-literal.key string.quoted.double",
            "settings": {
                "fontStyle": "underline",
                "foreground": "#0AF"
            }
        },
        {
            "scope": "meta.definition.property variable.object.property",
            "settings": {
                "fontStyle": "underline",
                "foreground": "#0AF"
            }
        },
        {
            "scope": "punctuation",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#777"
            }
        },
        {
            "scope": "meta.brace",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#777"
            }
        },
        {
            "scope": "string.regexp",
            "settings": {
                "foreground": "#F80"
            }
        },
        {
            "scope": "punctuation.definition.character-class.regexp",
            "settings": {
                "foreground": "#777"
            }
        },
        {
            "scope": "constant.other.character-class.range.regexp",
            "settings": {
                "foreground": "#777"
            }
        },
        {
            "scope": "string",
            "settings": {
                "foreground": "#F80"
            }
        },
        {
            "scope": "constant.character",
            "settings": {
                "foreground": "#F80"
            }
        },
        {
            "scope": "constant.character.escape",
            "settings": {
                "foreground": "#777"
            }
        },
        {
            "scope": "punctuation.definition.string",
            "settings": {
                "foreground": "#F80"
            }
        },
        {
            "scope": "punctuation.definition.template-expression",
            "settings": {
                "foreground": "#F80"
            }
        },
        {
            "scope": "entity.name.tag",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#777"
            }
        },
        {
            "scope": "text",
            "settings": {
                "foreground": "#777"
            }
        },
        {
            "scope": "entity.name.type",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#0C3"
            }
        },
        {
            "scope": "support.type",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#0C3"
            }
        },
        {
            "scope": "meta.type.declaration entity.name.type",
            "settings": {
                "fontStyle": "bold underline",
                "foreground": "#0C3"
            }
        },
        {
            "scope": "support.type.primitive",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#777"
            }
        },
        {
            "scope": "variable.other",
            "settings": {
                "foreground": "#F0F"
            }
        },
        {
            "scope": "support.constant",
            "settings": {
                "foreground": "#F0F"
            }
        },
        {
            "scope": "support.variable",
            "settings": {
                "foreground": "#F0F"
            }
        },
        {
            "scope": "meta.definition.variable variable.other",
            "settings": {
                "fontStyle": "underline",
                "foreground": "#F0F"
            }
        }
    ]
}