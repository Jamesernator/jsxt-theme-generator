{
    "$schema": "vscode://schemas/color-theme",
    "name": "JSXT Light",
    "type": "light",
    "colors": {
        "focusBorder": "#888",
        "foreground": "#444",
        "widget.shadow": "#555",
        "selection.background": "#TODO_REPLACE_BLUE",
        "activityBar.activeBackground": "#FFF",
        "activityBar.background": "#CCC",
        "activityBar.foreground": "#000",
        "activityBar.activeBorder": "#TODO_REPLACE_BLUE",
        "editor.background": "#EEE",
        "editor.foreground": "#222",
        "list.activeSelectionBackground": "#TODO_REPLACE_BLUE",
        "statusBar.background": "#TODO_REPLACE_BLUE",
        "statusBar.foreground": "#FFF",
        "terminal.background": "#EEE",
        "terminal.foreground": "#333",
        "terminalCursor.background": "#FFF",
        "terminalCursor.foreground": "#AAA",
        "terminal.ansiBlack": "#444",
        "terminal.ansiBlue": "#00F",
        "terminal.ansiCyan": "#0AA",
        "terminal.ansiGreen": "#0A0",
        "terminal.ansiMagenta": "#F0F",
        "terminal.ansiRed": "#C00",
        "terminal.ansiWhite": "#ffe9e9",
        "terminal.ansiYellow": "#BB0",
        "terminal.ansiBrightBlack": "#777",
        "terminal.ansiBrightBlue": "#TODO_REPLACE_BLUE",
        "terminal.ansiBrightCyan": "#0CC",
        "terminal.ansiBrightGreen": "#0C3",
        "terminal.ansiBrightMagenta": "#C0C",
        "terminal.ansiBrightRed": "#F00",
        "terminal.ansiBrightWhite": "#FFF",
        "terminal.ansiBrightYellow": "#CC0"
    },
    "semanticHighlighting": true,
    "semanticTokenColors": {
        "class": {
            "foreground": "#0AF",
            "bold": true
        },
        "class.declaration": {
            "foreground": "#0AF",
            "bold": true,
            "underline": true
        },
        "enum": {
            "foreground": "#0AF",
            "bold": true
        },
        "enum.declaration": {
            "foreground": "#0AF",
            "bold": true,
            "underline": true
        },
        "namespace": {
            "foreground": "#0AF",
            "bold": true
        },
        "namespace.declaration": {
            "foreground": "#0AF",
            "bold": true,
            "underline": true
        },
        "type": {
            "foreground": "#0AF",
            "bold": true
        },
        "type.declaration": {
            "foreground": "#0AF",
            "bold": true,
            "underline": true
        },
        "type.primitive": {
            "foreground": "#940"
        },
        "typeParameter": {
            "foreground": "#0AF",
            "bold": true
        },
        "typeParameter.declaration": {
            "foreground": "#0AF",
            "bold": true,
            "underline": true
        },
        "interface": {
            "foreground": "#0AF",
            "bold": true
        },
        "interface.declaration": {
            "foreground": "#0AF",
            "bold": true,
            "underline": true
        },
        "function": {
            "foreground": "#F0A"
        },
        "function.declaration": {
            "foreground": "#F0A",
            "underline": true
        },
        "method": {
            "foreground": "#F0A"
        },
        "method.declaration": {
            "foreground": "#F0A",
            "underline": true
        },
        "variable": {
            "foreground": "#0B0"
        },
        "variable.declaration": {
            "foreground": "#0B0",
            "underline": true
        },
        "parameter": {
            "foreground": "#0B0"
        },
        "parameter.declaration": {
            "foreground": "#0B0",
            "underline": true
        },
        "property": {
            "foreground": "#AA0"
        },
        "property.declaration": {
            "foreground": "#AA0",
            "underline": true
        },
        "enumMember": {
            "foreground": "#AA0"
        },
        "enumMember.declaration": {
            "foreground": "#AA0",
            "underline": true
        },
        "comment": {
            "foreground": "#AAA"
        },
        "keyword": {
            "foreground": "#A0F",
            "bold": true
        },
        "keyword.operator": {
            "foreground": "#77C",
            "bold": true
        },
        "keyword.expression": {
            "foreground": "#A0F"
        },
        "keyword.storage": {
            "foreground": "#TODO_REPLACE_BLUE",
            "bold": true,
            "italic": true
        },
        "boolean": {
            "foreground": "#00A"
        },
        "number": {
            "foreground": "#00A"
        },
        "constant": {
            "foreground": "#00A"
        },
        "label": {
            "foreground": "#F88"
        },
        "string": {
            "foreground": "#AA0"
        },
        "string.escape": {
            "foreground": "#F0A"
        },
        "regexp": {
            "foreground": "#AA0"
        },
        "regexp.characterClass": {
            "foreground": "#TODO_REPLACE_BLUE"
        },
        "operator": {
            "foreground": "#77C",
            "bold": true
        },
        "operator.assignment": {
            "foreground": "#A0C",
            "bold": true
        },
        "punctuation": {
            "foreground": "#888"
        },
        "text": {
            "foreground": "#A0C"
        },
        "tag": {
            "foreground": "#TODO_REPLACE_BLUE",
            "bold": true
        },
        "attribute": {
            "foreground": "#0C0"
        }
    }
}
