{
    "$schema": "vscode://schemas/color-theme",
    "name": "JSXT Light",
    "type": "light",
    "colors": {
        "focusBorder": "#888",
        "foreground": "#444",
        "widget.shadow": "#555",
        "selection.background": "#0AF",
        "activityBar.activeBackground": "#FFF",
        "activityBar.background": "#CCC",
        "activityBar.foreground": "#000",
        "activityBar.activeBorder": "#0AF",
        "editor.background": "#EEE",
        "editor.foreground": "#222",
        "list.activeSelectionBackground": "#0AF",
        "statusBar.background": "#0AF",
        "statusBar.foreground": "#FFF",
        "terminal.background": "#EEE",
        "terminal.foreground": "#333",
        "terminalCursor.background": "#FFF",
        "terminalCursor.foreground": "#AAA",
        "terminal.ansiBlack": "#444",
        "terminal.ansiBlue": "#00F",
        "terminal.ansiCyan": "#0AA",
        "terminal.ansiGreen": "#0A0",
        "terminal.ansiMagenta": "#F0F",
        "terminal.ansiRed": "#C00",
        "terminal.ansiWhite": "#ffe9e9",
        "terminal.ansiYellow": "#BB0",
        "terminal.ansiBrightBlack": "#777",
        "terminal.ansiBrightBlue": "#0AF",
        "terminal.ansiBrightCyan": "#0CC",
        "terminal.ansiBrightGreen": "#0C3",
        "terminal.ansiBrightMagenta": "#C0C",
        "terminal.ansiBrightRed": "#F00",
        "terminal.ansiBrightWhite": "#FFF",
        "terminal.ansiBrightYellow": "#CC0"
    },
    "semanticHighlighting": true,
    "semanticTokenColors": {
        "class": {
            "foreground": "#088",
            "bold": true
        },
        "class.declaration": {
            "foreground": "#088",
            "bold": true,
            "underline": true
        },
        "comment": {
            "foreground": "#AAA"
        },
        "constant": {
            "foreground": "#00A"
        },
        "enum": {
            "foreground": "#F0A",
            "bold": true
        },
        "enumMember": {
            "foreground": "#0A0"
        },
        "event": {
            "foreground": "#0FF"
        },
        "function": {
            "foreground": "#0AA"
        },
        "function.declaration": {
            "foreground": "#0AA",
            "underline": true
        },
        "interface": {
            "foreground": "#F0A",
            "bold": true
        },
        "keyword": {
            "foreground": "#A0F",
            "bold": true,
            "italic": true
        },
        "keyword.storage": {
            "foreground": "#0AF",
            "bold": true
        },
        "label": {
            "foreground": "#F00"
        },
        "macro": {
            "foreground": "#0AA"
        },
        "method": {
            "foreground": "#0AA"
        },
        "method.declaration": {
            "foreground": "#0AA",
            "underline": true
        },
        "namespace": {
            "foreground": "#0AA",
            "bold": true
        },
        "namespace.declaration": {
            "foreground": "#0AA",
            "bold": true,
            "underline": true
        },
        "number": {
            "foreground": "#00C"
        },
        "operator": {
            "foreground": "#F90",
            "bold": true
        },
        "parameter": {
            "foreground": "#0A0"
        },
        "parameter.declaration": {
            "foreground": "#0A0",
            "underline": true
        },
        "property": {
            "foreground": "#AA0"
        },
        "property.declaration": {
            "foreground": "#AA0",
            "underline": true
        },
        "regexp": {
            "foreground": "#A09"
        },
        "regexp.characterClass": {
            "foreground": "#0AF"
        },
        "string": {
            "foreground": "#0CC",
            "bold": true
        },
        "string.escape": {
            "foreground": "#0AF",
            "bold": true
        },
        "string.terminator": {
            "foreground": "#955"
        },
        "struct": {
            "foreground": "#F00"
        },
        "tomlArrayKey": {
            "foreground": "#F00"
        },
        "tomlTableKey": {
            "foreground": "#F00"
        },
        "type": {
            "foreground": "#F0A",
            "bold": true
        },
        "type.declaration": {
            "foreground": "#F0A",
            "bold": true,
            "underline": true
        },
        "type.primitive": {
            "foreground": "#A00",
            "italic": true
        },
        "typeParameter": {
            "foreground": "#55F"
        },
        "variable": {
            "foreground": "#0A0"
        },
        "variable.defaultLibrary": {
            "foreground": "#0A0",
            "italic": true,
            "bold": true
        }
    },
    "tokenColors": [
        {
            "scope": "entity.name.type.class",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#088"
            }
        },
        {
            "scope": "meta.class entity.other.inherited-class",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#088"
            }
        },
        {
            "scope": "meta.class entity.name.type.class",
            "settings": {
                "fontStyle": "bold underline",
                "foreground": "#088"
            }
        },
        {
            "scope": "comment",
            "settings": {
                "foreground": "#AAA"
            }
        },
        {
            "scope": "constant.language",
            "settings": {
                "foreground": "#00A"
            }
        },
        {
            "scope": "entity.name.type.enum",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#F0A"
            }
        },
        {
            "scope": "meta.enum.declaration entity.name.type.enum",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#F0A"
            }
        },
        {
            "scope": "variable.other.enummember",
            "settings": {
                "foreground": "#0A0"
            }
        },
        {
            "scope": "meta.enum.declaration variable.other.enummember",
            "settings": {
                "foreground": "#0A0"
            }
        },
        {
            "scope": "entity.name.function",
            "settings": {
                "foreground": "#0AA"
            }
        },
        {
            "scope": "meta.definition.function entity.name.function",
            "settings": {
                "fontStyle": "underline",
                "foreground": "#0AA"
            }
        },
        {
            "scope": "entity.name.type.interface",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#F0A"
            }
        },
        {
            "scope": "meta.interface entity.other.inherited-class",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#F0A"
            }
        },
        {
            "scope": "meta.interface entity.name.type.interface",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#F0A"
            }
        },
        {
            "scope": "keyword.other",
            "settings": {
                "fontStyle": "bold italic",
                "foreground": "#A0F"
            }
        },
        {
            "scope": "variable.language",
            "settings": {
                "fontStyle": "bold italic",
                "foreground": "#A0F"
            }
        },
        {
            "scope": "keyword.other.new",
            "settings": {
                "fontStyle": "bold italic",
                "foreground": "#A0F"
            }
        },
        {
            "scope": "keyword.operator.expression",
            "settings": {
                "fontStyle": "bold italic",
                "foreground": "#A0F"
            }
        },
        {
            "scope": "keyword.storage",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#0AF"
            }
        },
        {
            "scope": "keyword.control",
            "settings": {
                "fontStyle": "bold italic",
                "foreground": "#A0F"
            }
        },
        {
            "scope": "entity.name.label",
            "settings": {
                "foreground": "#F00"
            }
        },
        {
            "scope": "meta.method.declaration meta.definition.method entity.name.function",
            "settings": {
                "fontStyle": "underline",
                "foreground": "#0AA"
            }
        },
        {
            "scope": "entity.name.type.module",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#0AA"
            }
        },
        {
            "scope": "meta.namespace.declaration entity.name.type.module",
            "settings": {
                "fontStyle": "bold underline",
                "foreground": "#0AA"
            }
        },
        {
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#00C"
            }
        },
        {
            "scope": "keyword.operator",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#F90"
            }
        },
        {
            "scope": "punctuation.separator",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#F90"
            }
        },
        {
            "scope": "punctuation.definition.group",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#F90"
            }
        },
        {
            "scope": "keyword.operator.assignment",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#F90"
            }
        },
        {
            "scope": "keyword.operator.bitwise",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#F90"
            }
        },
        {
            "scope": "keyword.operator.logical",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#F90"
            }
        },
        {
            "scope": "keyword.operator.arithmetic",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#F90"
            }
        },
        {
            "scope": "keyword.operator.comparison",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#F90"
            }
        },
        {
            "scope": "variable.parameter",
            "settings": {
                "foreground": "#0A0"
            }
        },
        {
            "scope": "meta.parameters variable.parameter",
            "settings": {
                "fontStyle": "underline",
                "foreground": "#0A0"
            }
        },
        {
            "scope": "variable.other.property",
            "settings": {
                "foreground": "#AA0"
            }
        },
        {
            "scope": "variable.object.property",
            "settings": {
                "foreground": "#AA0"
            }
        },
        {
            "scope": "support.variable.property",
            "settings": {
                "foreground": "#AA0"
            }
        },
        {
            "scope": "meta.object-literal.key",
            "settings": {
                "fontStyle": "underline",
                "foreground": "#AA0"
            }
        },
        {
            "scope": "meta.object-literal.key string.quoted.double",
            "settings": {
                "fontStyle": "underline",
                "foreground": "#AA0"
            }
        },
        {
            "scope": "meta.definition.property variable.object.property",
            "settings": {
                "fontStyle": "underline",
                "foreground": "#AA0"
            }
        },
        {
            "scope": "string.regexp",
            "settings": {
                "foreground": "#A09"
            }
        },
        {
            "scope": "punctuation.definition.character-class.regexp",
            "settings": {
                "foreground": "#0AF"
            }
        },
        {
            "scope": "constant.other.character-class.range.regexp",
            "settings": {
                "foreground": "#0AF"
            }
        },
        {
            "scope": "string",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#0CC"
            }
        },
        {
            "scope": "constant.character.escape",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#0AF"
            }
        },
        {
            "scope": "punctuation.definition.string",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#0CC"
            }
        },
        {
            "scope": "punctuation.definition.template-expression",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#0CC"
            }
        },
        {
            "scope": "entity.name.type",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#F0A"
            }
        },
        {
            "scope": "support.type",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#F0A"
            }
        },
        {
            "scope": "meta.type.declaration entity.name.type",
            "settings": {
                "fontStyle": "bold underline",
                "foreground": "#F0A"
            }
        },
        {
            "scope": "support.type.primitive",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#A00"
            }
        },
        {
            "scope": "variable.other",
            "settings": {
                "foreground": "#0A0"
            }
        },
        {
            "scope": "support.constant",
            "settings": {
                "foreground": "#0A0"
            }
        },
        {
            "scope": "support.variable",
            "settings": {
                "foreground": "#0A0"
            }
        },
        {
            "scope": "meta.definition.variable variable.other",
            "settings": {
                "foreground": "#0A0"
            }
        }
    ]
}