{
    "$schema": "vscode://schemas/color-theme",
    "name": "JSXT Light",
    "type": "light",
    "colors": {
        "focusBorder": "#888",
        "foreground": "#444",
        "widget.shadow": "#555",
        "selection.background": "#0AF",
        "activityBar.activeBackground": "#FFF",
        "activityBar.background": "#CCC",
        "activityBar.foreground": "#000",
        "activityBar.activeBorder": "#0AF",
        "editor.background": "#EEE",
        "editor.foreground": "#222",
        "list.activeSelectionBackground": "#0AF",
        "statusBar.background": "#0AF",
        "statusBar.foreground": "#FFF",
        "terminal.background": "#EEE",
        "terminal.foreground": "#333",
        "terminalCursor.background": "#FFF",
        "terminalCursor.foreground": "#AAA",
        "terminal.ansiBlack": "#444",
        "terminal.ansiBlue": "#00F",
        "terminal.ansiCyan": "#0AA",
        "terminal.ansiGreen": "#0A0",
        "terminal.ansiMagenta": "#F0F",
        "terminal.ansiRed": "#C00",
        "terminal.ansiWhite": "#ffe9e9",
        "terminal.ansiYellow": "#BB0",
        "terminal.ansiBrightBlack": "#777",
        "terminal.ansiBrightBlue": "#0AF",
        "terminal.ansiBrightCyan": "#0CC",
        "terminal.ansiBrightGreen": "#0C3",
        "terminal.ansiBrightMagenta": "#C0C",
        "terminal.ansiBrightRed": "#F00",
        "terminal.ansiBrightWhite": "#FFF",
        "terminal.ansiBrightYellow": "#CC0"
    },
    "semanticHighlighting": true,
    "semanticTokenColors": {
        "class": {
            "foreground": "#088",
            "bold": true
        },
        "class.declaration": {
            "foreground": "#088",
            "bold": true,
            "underline": true
        },
        "comment": {
            "foreground": "#AAA"
        },
        "enum": {
            "foreground": "#F0A",
            "bold": true
        },
        "enumMember": {
            "foreground": "#0A0"
        },
        "event": {
            "foreground": "#0FF"
        },
        "function": {
            "foreground": "#0AA"
        },
        "function.declaration": {
            "foreground": "#0AA",
            "underline": true
        },
        "interface": {
            "foreground": "#F0A",
            "bold": true
        },
        "keyword": {
            "foreground": "#A0F",
            "bold": true,
            "italic": true
        },
        "keyword.storage": {
            "foreground": "#0AF",
            "bold": true
        },
        "label": {
            "foreground": "#F00"
        },
        "macro": {
            "foreground": "#0AA"
        },
        "method": {
            "foreground": "#0AA"
        },
        "method.declaration": {
            "foreground": "#0AA",
            "underline": true
        },
        "namespace": {
            "foreground": "#0AA",
            "bold": true
        },
        "namespace.declaration": {
            "foreground": "#0AA",
            "bold": true,
            "underline": true
        },
        "number": {
            "foreground": "#00C"
        },
        "operator": {
            "foreground": "#F90",
            "bold": true
        },
        "parameter": {
            "foreground": "#0A0"
        },
        "parameter.declaration": {
            "foreground": "#0A0",
            "underline": true
        },
        "property": {
            "foreground": "#AA0"
        },
        "property.declaration": {
            "foreground": "#AA0",
            "underline": true
        },
        "regexp": {
            "foreground": "#A09"
        },
        "regexp.characterClass": {
            "foreground": "#0AF"
        },
        "string": {
            "foreground": "#0CC",
            "bold": true
        },
        "string.escape": {
            "foreground": "#0AF",
            "bold": true
        },
        "string.terminator": {
            "foreground": "#955"
        },
        "struct": {
            "foreground": "#F00"
        },
        "tomlArrayKey": {
            "foreground": "#F00"
        },
        "tomlTableKey": {
            "foreground": "#F00"
        },
        "type": {
            "foreground": "#F0A",
            "bold": true
        },
        "type.declaration": {
            "foreground": "#F0A",
            "bold": true,
            "underline": true
        },
        "type.primitive": {
            "foreground": "#A00",
            "italic": true
        },
        "typeParameter": {
            "foreground": "#55F"
        },
        "variable": {
            "foreground": "#0A0"
        },
        "variable.defaultLibrary": {
            "foreground": "#0A0",
            "italic": true,
            "bold": true
        }
    },
    "tokenColors": [
        {
            "scope": [
                "entity.other.inherited-class, meta.class"
            ],
            "settings": {
                "foreground": "#088",
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "entity.name.type.class, meta.class"
            ],
            "settings": {
                "foreground": "#088",
                "fontStyle": "bold underline"
            }
        },
        {
            "scope": [
                "comment"
            ],
            "settings": {
                "foreground": "#AAA",
                "fontStyle": ""
            }
        },
        {
            "scope": [
                "entity.name.type.enum"
            ],
            "settings": {
                "foreground": "#F0A",
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "variable.other.enummember"
            ],
            "settings": {
                "foreground": "#0A0",
                "fontStyle": ""
            }
        },
        {
            "scope": [],
            "settings": {
                "foreground": "#0FF",
                "fontStyle": ""
            }
        },
        {
            "scope": [
                "entity.name.function"
            ],
            "settings": {
                "foreground": "#0AA",
                "fontStyle": ""
            }
        },
        {
            "scope": [],
            "settings": {
                "foreground": "#0AA",
                "fontStyle": "underline"
            }
        },
        {
            "scope": [
                "entity.name.type.interface"
            ],
            "settings": {
                "foreground": "#F0A",
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "keyword.control",
                "keyword.operator.expression",
                "keyword.other",
                "variable.language"
            ],
            "settings": {
                "foreground": "#A0F",
                "fontStyle": "bold italic"
            }
        },
        {
            "scope": [
                "storage"
            ],
            "settings": {
                "foreground": "#0AF",
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "entity.name.label"
            ],
            "settings": {
                "foreground": "#F00",
                "fontStyle": ""
            }
        },
        {
            "scope": [],
            "settings": {
                "foreground": "#0AA",
                "fontStyle": ""
            }
        },
        {
            "scope": [],
            "settings": {
                "foreground": "#0AA",
                "fontStyle": ""
            }
        },
        {
            "scope": [],
            "settings": {
                "foreground": "#0AA",
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "entity.name.type.module"
            ],
            "settings": {
                "foreground": "#0AA",
                "fontStyle": "bold underline"
            }
        },
        {
            "scope": [
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#00C",
                "fontStyle": ""
            }
        },
        {
            "scope": [
                "keyword.operator",
                "punctuation.definition.group"
            ],
            "settings": {
                "foreground": "#F90",
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "variable.parameter"
            ],
            "settings": {
                "foreground": "#0A0",
                "fontStyle": ""
            }
        },
        {
            "scope": [],
            "settings": {
                "foreground": "#0A0",
                "fontStyle": "underline"
            }
        },
        {
            "scope": [
                "variable.other.property",
                "variable.object.property"
            ],
            "settings": {
                "foreground": "#AA0",
                "fontStyle": ""
            }
        },
        {
            "scope": [
                "string.regexp"
            ],
            "settings": {
                "foreground": "#A09",
                "fontStyle": ""
            }
        },
        {
            "scope": [
                "punctuation.definition.character-class.regexp",
                "constant.other.character-class.range.regexp"
            ],
            "settings": {
                "foreground": "#0AF",
                "fontStyle": ""
            }
        },
        {
            "scope": [
                "string"
            ],
            "settings": {
                "foreground": "#0CC",
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "constant.character.escape"
            ],
            "settings": {
                "foreground": "#0AF",
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "punctuation.definition.string",
                "punctuation.definition.template-expression"
            ],
            "settings": {
                "foreground": "#955",
                "fontStyle": ""
            }
        },
        {
            "scope": [],
            "settings": {
                "foreground": "#F00",
                "fontStyle": ""
            }
        },
        {
            "scope": [],
            "settings": {
                "foreground": "#F00",
                "fontStyle": ""
            }
        },
        {
            "scope": [],
            "settings": {
                "foreground": "#F00",
                "fontStyle": ""
            }
        },
        {
            "scope": [
                "entity.name.type.alias",
                "support.type"
            ],
            "settings": {
                "foreground": "#F0A",
                "fontStyle": "bold"
            }
        },
        {
            "scope": [],
            "settings": {
                "foreground": "#F0A",
                "fontStyle": "bold underline"
            }
        },
        {
            "scope": [
                "support.type.primitive"
            ],
            "settings": {
                "foreground": "#A00",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "entity.name.type"
            ],
            "settings": {
                "foreground": "#55F",
                "fontStyle": ""
            }
        },
        {
            "scope": [
                "variable.other"
            ],
            "settings": {
                "foreground": "#0A0",
                "fontStyle": ""
            }
        }
    ]
}
